<div id="restreamer_wrapper">
	<iframe data-bind="attr: {src: view_url()}" frameborder="0" allowfullscreen></iframe>
</div>
<div class="row-fluid btn-group" style="text-align: center;padding-top: 20px;" data-bind="visible: settingsViewModel.settings.plugins.restreamer.host().length > 0">
	<button class="btn" data-bind="click: toggleStream, css: btnclass, disable:processing">
        <i data-bind="css: icon"></i>
        <span data-bind="text: streaming() ? '  Stop' : '  Go Live'"></span>
    </button>
</div>
